        tbody tr td:nth-child(7): 'background-color: green; color: white;'
      card_mod:
        style:
          .: |
            ha-card {
              overflow: auto;
              }
          $: |
            .card-header {
               padding-top: 6px!important;
               padding-bottom: 4px!important;
               font-size: 14px!important;
               line-height: 14px!important;
               font-weight: bold!important;
             }
      sort_by: events+
      entities:
        include: '[[entity]]'
        exclude: '[[excluded_entities]]'
      columns:
        - name: Game Time
          data: '[[events]]'
          modify: |-
            (() => {
               if (x?.date) {
                 const utcDate = new Date(x.date);
                 const formattedDateTime = utcDate.toLocaleString('en-US', {
                    month: '2-digit', 
                    day: '2-digit',
                    year: 'numeric',
                    hour: '2-digit',
                    minute: '2-digit',
                    hour12: true 
                 });
                 return formattedDateTime;
               } else {
                 return 'Date/Time not available'; 
               }
            })()
        - name: Headline
          data: '[[events]]'
          modify: x.competitions[0].notes[0].headline
        - name: Home Team
          data: '[[events]]'
          modify: >-
            '<div><img src="' + x.competitions[0].competitors[0].team.logo + '"
            style="height: 20px;vertical-align:middle;">&nbsp;' +
            x.competitions[0].competitors[0].team.displayName + "(" +
            x.competitions[0].competitors[0].curatedRank.current + ")"  +
            '</div>'
        - name: Away Team
          data: '[[events]]'
          modify: >-
            '<div><img src="' + x.competitions[0].competitors[1].team.logo + '"
            style="height: 20px;vertical-align:middle;">&nbsp;' +
            x.competitions[0].competitors[1].team.displayName + "(" +
            x.competitions[0].competitors[1].curatedRank.current + ")"  +
            '</div>'       
        - name: Final Score (Away-Home)
          data: '[[events]]'
          modify: |-
            if (typeof x !== 'undefined' &&
                typeof x.competitions[0].competitors[0].score !== 'undefined' &&
                typeof x.competitions[0].competitors[1].score !== 'undefined') {
              let content = '';
              let icon = '<ha-icon icon="mdi:minus" style="fill:white;"></ha-icon>';
              let home_score = x.competitions[0].competitors[0].score;
              let away_score = x.competitions[0].competitors[1].score;


              if (home_score > away_score) {
                content = `<div style="font-size:20px;color:red;">${away_score}</div>` +
                          `<div style="font-size:20px;color:black;margin:0 5px;">-</div>` +
                          `<div style="font-size:20px;color:green;">${home_score}</div>`;
              } else if (home_score < away_score) {
                content = `<div style="font-size:20px;color:green;">${away_score}</div>` +
                          `<div style="font-size:20px;color:black;margin:0 5px;">-</div>` +
                          `<div style="font-size:20px;color:red;">${home_score}</div>`;
              } else {
                content = `<div style="font-size:20px;color:black;">${away_score}-${home_score}</div>`;
              }
                            
              content = `<div style="display:flex;align-items:center;justify-content:center;width:100%;height:100%;">` +
                content +
                `</div>`
                
              content;
            } else {
              '';
            }
  ncaa_standings:
    card:
      type: custom:flex-table-card
      title: '[[title]]'
      css:
        table+: 'padding: 0px; width: 1000px;'
        tbody tr td:first-child: 'width: 2%;'
        tbody tr td:nth-child(2): 'width: 5%;'
        tbody tr td:nth-child(n+3): 'width: 2%;'
        tbody tr:hover: 'background-color: black!important; color:white!important;'
        tbody tr td:nth-child(6): 'background-color: green; color: white;'
      card_mod:
        style:
          .: |
            ha-card {
              overflow: auto;
              }
          $: |
            .card-header {
               padding-top: 6px!important;
               padding-bottom: 4px!important;
               font-size: 14px!important;
               line-height: 14px!important;
               font-weight: bold!important;
             }
      sort_by:
        - SEED+
      entities:
        include: '[[entity]]'
      columns:
        - name: SEED
          data: entries
          modify: x.stats.find(y=>y.abbreviation == 'SEED').displayValue
          hidden: true
        - name: Seed
          data: entries
          modify: x.stats.find(y=>y.abbreviation == 'SEED').displayValue
        - name: Team with rank
          data: entries
          modify: |-
            if (x.team.rank <= 25) {
              '<div><img src="' + x.team.logos[0].href + '" style="height: 20px;vertical-align:middle;">&nbsp;' + x.team.displayName + "(" + x.team.rank +")" + '</div>' 
            } else {
               '<div><img src="' + x.team.logos[0].href + '" style="height: 20px;vertical-align:middle;">&nbsp;' + x.team.displayName + '</div>' 
            }
        - name: Overall
          data: entries
          modify: >-
            if(typeof x.stats.find(y=>y.abbreviation == 'overall') !==
            'undefined' ){x.stats.find(y=>y["abbreviation"] ==
            'overall').displayValue}else{'-'}
        - name: GB
          data: entries
          modify: >-
            if(typeof x.stats.find(y=>y.abbreviation == 'GB') !== 'undefined'
            ){x.stats.find(y=>y["abbreviation"] == 'GB').displayValue}else{'-'}
        - name: Wins-Losses
          data: entries
          modify: >-
            if(typeof x.stats.find(y=>y.name == 'overall') !== 'undefined'
            ){x.stats.find(y=>y["name"] == 'overall').displayValue}else{'-'}
        - name: Conf Record
          data: entries
          modify: >-
            if(typeof x.stats.find(y=>y.abbreviation == 'CONF') !== 'undefined'
            ){x.stats.find(y=>y["abbreviation"] ==
            'CONF').displayValue}else{'-'}
        - name: Streak
          data: entries
          modify: >-
            if(typeof x.stats.find(y=>y.abbreviation == 'STRK') !== 'undefined'
            ){x.stats.find(y=>y["abbreviation"] == 'STRK').value}else{'-'}
