        rankings %}
          {% if ranking.CFP_ranking | int == [[CFP_ranking]] %}
            {{ ranking.CFP_ranking }}. {{ ranking.team.nickname }} {{ ranking.team.name }}
          {% endif %}
        {% endfor %} 
      card_mod:
        style: |
          ha-card {
            {% set rankings = state_attr('[[entity]]', 'teams') %}
            {% for ranking in rankings %}
              {% if ranking.CFP_ranking | int == [[CFP_ranking]] %}
                background: url({{ ranking.team.logos[0].href }}) no-repeat right center;
                background-size: contain;
                color: white;
                border: 2px solid #{{ ranking.team.color }};
                border-radius: 10px;
                padding: 10px;
              {% endif %}
            {% endfor %}
          }
