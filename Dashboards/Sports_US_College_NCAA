        template: >
          {%- set top_25_teams =
          state_attr('[[team_names]]','team_names').split(', ') -%}

          {%- for team in integration_entities('teamtracker') -%}
            {%- set team_abbr = state_attr(team, 'team_abbr') -%}
            {%- set opponent_abbr = state_attr(team, 'opponent_abbr') -%}
            {%- set team_homeaway = state_attr(team, 'team_homeaway') -%}
            
            {%- if state_attr(team, 'league') == '[[sport]]' -%}

              {%- if team_abbr in top_25_teams and team_homeaway == "home" and opponent_abbr in top_25_teams -%}                                                          
                {{{"type": "custom:teamtracker-card","entity": team,"card_mod": {"style": "ha-card { font-size: .7em; line-height: .7em; width: 280px; border: 2px solid red; background-color: rgba(224, 247, 250, 0.1); }"},
                   "home_side": "right",}}},
              {%- elif team_abbr in top_25_teams and team_homeaway == "home" and opponent_abbr not in top_25_teams -%}  
                {{{"type": "custom:teamtracker-card","entity": team,"card_mod": {"style": "ha-card { font-size: .7em; line-height: .7em; width: 280px; border: 2px solid blue; background-color: rgba(224, 247, 250, 0.1); }"},
                "home_side": "right",}}},
              {%- elif team_abbr in top_25_teams and team_homeaway != "home" and opponent_abbr not in top_25_teams  -%}      
                {{{"type": "custom:teamtracker-card","entity": team,"card_mod": {"style": "ha-card { font-size: .7em; line-height: .7em; width: 280px; border: 2px solid green; background-color: rgba(224, 247, 250, 0.1); }"},
                 "home_side": "right",}}},
              {%- endif -%}
            
            {%- endif -%}
          {%- endfor -%}
        exclude:
          - entity_id: '*team_tracker*'
      sort:
        method: attribute
        attribute: date
